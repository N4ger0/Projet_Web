{% if app.user == null %}
    <img id="img" src="{{ asset('BangerMovies.png') }}"/>
{% elseif app.user.roles == ['ROLE_CLIENT'] %}
    <img id="img" src="{{ asset('BangerMoviesClient.png') }}"/>
{% elseif app.user.roles == ['ROLE_ADMIN'] %}
    <img id="img"  src="{{ asset('BangerMoviesAdmin.png') }}"/>
{% elseif app.user.roles == ['ROLE_SADMIN'] %}
    <img id="img"  src="{{ asset('BangerMoviesSadmin.png') }}"/>
{% endif %}